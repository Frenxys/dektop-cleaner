{
  "name": "dektop-cleaner",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "parcel build index.html --dist-dir dist --public-url ./",
    "dev": "parcel index.html --open"
  },
  "targets": {
    "main": {
      "includeNodeModules": true,
      "context": "node",
      "isLibrary": true,
      "outputFormat": "commonjs",
